package aplicacao;

import javax.persistence.EntityManager;
import javax.persistence.EntityManagerFactory;
import javax.persistence.Persistence;

import dominio.Pessoa;

public class Programa {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa(null, "Alessandro Pericles", "aaaaaaa@gmail.com");
		Pessoa p2 = new Pessoa(null, "Patricia Vieira", "bbbbbbb@gmail.com");
		Pessoa p3 = new Pessoa(null, "Patrick Pericles", "ccccc@gmail.com");
		
		EntityManagerFactory factory = Persistence.createEntityManagerFactory("connection_postgreSQL");
		EntityManager em = factory.createEntityManager();
		
        System.out.println(p1);
        System.out.println(p2);
        System.out.println(p3);
	}

}
